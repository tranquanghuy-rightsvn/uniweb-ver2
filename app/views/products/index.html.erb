<div class="container">
  <table class="table table-striped table-bordered">
    <thead>
      <tr>
        <th scope="col">ID</th>
        <th scope="col">Title</th>
        <th scope="col">Action</th>
      </tr>
    </thead>
    <tbody>
      <% @products.map do |product| %>
        <tr>
          <th scope="row"><%= product.id %></th>
          <td><%= product.title %></td>
          <td><button class="btn btn-primary">Edit</button><button class="btn btn-danger">Delete</button></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <%= link_to "<button class='btn btn-success'> Táº¡o product</button>".html_safe, new_website_product_path(website_id: params[:website_id]) %>
</div>
